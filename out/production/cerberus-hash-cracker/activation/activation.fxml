<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="activation.ActivationController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="300.0">
         <children>
            <Label layoutX="69.0" layoutY="29.0" text="Insert your product key">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfKeyField" onAction="#btnActivate" alignment="CENTER" layoutX="23.0" layoutY="74.0" prefHeight="26.0" prefWidth="255.0" promptText="XXXX-XXXX-XXXX-XXXX" />
            <Label fx:id="lKeyStatus" alignment="CENTER" layoutX="42.0" layoutY="118.0" prefHeight="18.0" prefWidth="219.0" textFill="RED">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button layoutX="208.0" layoutY="149.0" mnemonicParsing="false" onAction="#btnActivate" text="Activate" />
            <Button layoutX="14.0" layoutY="149.0" mnemonicParsing="false" onAction="#btnCommunityEdition" text="Use community edition" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
